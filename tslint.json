{
  "defaultSeverity": "error",
  "extends": [
    "tslint:all"
  ],
  "jsRules": {},
  "rules": {
    "completed-docs": [
      true,
      {
        "methods": {
          "privacies": [
            "public"
          ]
        },
        "enums": true,
        "functions": {
          "visibilities": [
            "exported"
          ]
        }
      }
    ],
    "file-name-casing": [
      true,
      "kebab-case"
    ],
    "increment-decrement": false,
    "interface-name": false,
    "strict-boolean-expressions": false,
    "newline-per-chained-call": false,
    "no-magic-numbers": [
      true,
      0,
      1,
      2,
      -1,
      -2,
      10,
      12,
      16,
      60
    ],
    "only-arrow-functions": false,
    "quotemark": false,
    "typedef": [
      true,
      "call-signature",
      "parameter",
      "property-declaration",
      "member-variable-declaration",
      "object-destructuring",
      "array-destructuring"
    ]
  },
  "rulesDirectory": []
}