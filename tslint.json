{
    "defaultSeverity": "error",
    "extends": [
        "tslint:all"
    ],
    "jsRules": {},
    "rules": {
      "file-name-casing": [true, "kebab-case"],
      "increment-decrement": false,
      "newline-per-chained-call": false,
      "only-arrow-functions": false,
      "quotemark": false,
      "typedef": [true, "call-signature", "parameter","property-declaration","member-variable-declaration","object-destructuring","array-destructuring"]
    },
    "rulesDirectory": []
}